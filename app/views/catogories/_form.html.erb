<%= form_for(@catogory, :html=>{:class=>"nice"}) do |f| %>
  <%= render(:partial=>"shared/form_errors", :object=>@catogory) %>

  <%= f.label :name %>
  <%= f.text_field :name, :class => "input-text" %>
  <%= f.label :description %>
  <%= f.text_area :description, :class => "input-text" %>
  <%= f.submit %>
<% end %>
